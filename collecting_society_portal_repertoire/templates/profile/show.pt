<tal:block metal:use-macro="backend363" i18n:domain="collecting_society_portal_repertoire">

    <!-- content middle -->
    <tal:block metal:fill-slot="content-middle">

        <div class="cs-symbolnavbar">
            <a href="javascript: history.back(1);">
                <span class="glyphicon glyphicon-arrow-left"></span>
            </a>
            <a href="${request.resource_path(context, 'edit')}">
                <span class="glyphicon glyphicon-pencil"></span>
            </a>
        </div>

        <div class="cs-profile-show">
            <table class="table table-hover">

                <!-- 
                <tr>
                    <th i18n:translate="">Name</th>
                    <td>${webuser.party.name or ""}</td>
                </tr>
                -->

                <tr>
                    <th i18n:translate="">Firstname</th>
                    <td>${webuser.party.firstname or ""}</td>
                </tr>

                <tr>
                    <th i18n:translate="">Lastname</th>
                    <td>${webuser.party.lastname or ""}</td>
                </tr>

                <tr>
                    <th i18n:translate="">EMail</th>
                    <td>${webuser.email or ""}</td>
                </tr>

                <tr>
                    <th i18n:translate="">Create date</th>
                    <td>${webuser.create_date.strftime("%d.%m.%Y") or ""}</td>
                </tr>

                <!-- 
                <tr>
                    <th i18n:translate="">Password</th>
                    <td>${webuser.password}</td>
                </tr>
                -->

            </table>
        </div>

    </tal:block>

</tal:block>
